@page
@model AboutModel
@using RazorPagesWikipedia.DbModels;

@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@Model.Message</h3>

@{

    var db = Database.Open("enwiki_20180401");

        var selectedData = db.Query("select count(*), cl_to from Categorylinks group by ClTo having count(*) > 100000");
    
    var grid = new WebGrid(source: selectedData);
}

<div>
    @grid.GetHtml();
</div>

